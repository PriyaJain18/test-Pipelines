pipeline {
    agent any

    stages {
        stage('Begin Pipeline') {
            steps {
                echo 'Pipeline started'
            }
        }
        stage('Develop') {
            steps {
                echo 'Developing'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
